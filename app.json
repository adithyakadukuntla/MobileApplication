{
  "expo": {
    "name": "Auditorium Booking VNR",
    "slug": "auditorium-booking-vnr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vnr_images.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vnr_images.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.adithya_k.auditoriumbookingvnr"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/vnr_images.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4e9c21e0-99ba-4e05-8447-c6cef2934f5a"
      }
    }
  }
}
